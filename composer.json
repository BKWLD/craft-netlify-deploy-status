{
    "name": "bukwild/craft-netlify-deploy-status",
    "description": "Craft plugin that shows Netlify deploy statuses",
    "type": "craft-plugin",
    "version": "1.0.5",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "craft netlify deploy status"
    ],
    "support": {
        "docs": "https://github.com/BKWLD/craft-netlify-deploy-status/blob/master/README.md",
        "issues": "https://github.com/BKWLD/craft-netlify-deploy-status/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bukwild",
            "homepage": "https://bukwild.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "bukwild\\craftnetlifydeploystatus\\": "src/"
        }
    },
    "extra": {
        "name": "Netlify Status",
        "handle": "craft-netlify-deploy-status",
        "developer": "Bukwild",
        "developerUrl": "https://bukwild.com",
        "documentationUrl": "https://github.com/BKWLD/craft-netlify-deploy-status/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/BKWLD/craft-netlify-deploy-status/master/CHANGELOG.md",
        "class": "bukwild\\craftnetlifydeploystatus\\CraftNetlifyDeployStatus"
    }
}
